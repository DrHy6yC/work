<Window x:Name="LoginWin" x:Class="Final_wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Final_wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="300">
    <Grid>
        <Button x:Name="LoginButton" Content="Enter" HorizontalAlignment="Left" Margin="87,269,0,0" VerticalAlignment="Top" Width="119" Click="LoginButton_Click"/>
        <TextBox x:Name="LoginText" HorizontalAlignment="Left" Height="26" Margin="139,133,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="PassText" HorizontalAlignment="Left" Height="26" Margin="139,185,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <Label Content="login" HorizontalAlignment="Left" Margin="41,133,0,0" VerticalAlignment="Top"/>
        <Label Content="Pass" HorizontalAlignment="Left" Margin="41,185,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
